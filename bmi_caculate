 1 #include <stdio.h>  2 int main(void)  3 {  4         float bmi, weight, height;  5  6         printf("** 체중(kg)을 입력하세요 : ");  7         scanf("%f", &weight);  8         printf("** 키(cm)를 입력하세요 : ");  9         scanf("%f", &height); 10 11         height = height / 100; 12         bmi = weight / (height * height); 13 14         printf("***** 당신의 체질량지수는 : %.1f입니다.\n", bmi); 15 16         return 0; 17 18 }
